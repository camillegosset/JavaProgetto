<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.View.AccountSelectionController">
   <center>
      <ComboBox fx:id="userChoice" prefHeight="65.0" prefWidth="255.0" promptText="Choice login" visibleRowCount="3" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#pressBoutonOK" prefHeight="80.0" prefWidth="150.0" text="OK" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="30.0" />
         </BorderPane.margin></Button>
   </bottom>
   <top>
      <Label fx:id="label" prefHeight="87.0" prefWidth="180.0" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
